{
  "name": "ship-station",
  "vendor": "vtex",
  "version": "0.0.2",
  "title": "ShipStation",
  "description": "ShipStation API",
  "settingsSchema": {
    "title": "ShipStation",
    "type": "object",
    "properties": {
      "apiKey": {
        "title": "API Key",
        "description": "ShipStation API Key",
        "type": "string"
      },
      "apiSecret": {
        "title": "API Secret",
        "description": "ShipStation API Secret",
        "type": "string"
      },
      "storeName": {
        "title": "Store Name",
        "description": "ShipStation Store Name (optional)",
        "type": "string"
      },
      "weightUnit": {
        "title": "Weight Unit",
        "description": "Unit of weight measure",
        "type": "string",
        "enum": [
          "pounds",
          "ounces",
          "grams"
        ],
        "enumNames": [
          "pounds",
          "ounces",
          "grams"
        ]
      },
      "splitShipmentByLocation": {
        "title": "Split Shipments",
        "description": "Split Shipments By Location",
        "type": "boolean"
      },
      "sendPickupInStore": {
        "title": "Send Pickup In Store",
        "description": "Send Pickup In Store Orders to ShipStation",
        "type": "boolean"
      },
      "marketplaceOnly": {
        "title": "Marketplace Only",
        "description": "Only send Marketplace Orders",
        "type": "boolean"
      }
    }
  },
  "builders": {
    "dotnet": "2.x",
    "docs": "0.x"
  },
  "mustUpdateAt": "2018-09-05",
  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "registries": [
    "smartcheckout"
  ],
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "bnb.data.bl.uk"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "nuget.org"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/oms/*"
      }
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "AcessaTodosPedidos"
    },
    {
      "name": "OMSViewer"
    },
    {
      "name": "ShippingAction"
    },
    {
      "name": "WorkflowAction"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/checkout/pvt/orders/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/orders/hook/config"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "ssapi.shipstation.com",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "ssapi12.shipstation.com",
        "path": "shipments"
      }
    },
    {
      "name": "vbase-read-write"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
