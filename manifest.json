{
  "name": "ship-station",
  "vendor": "vtex",
  "version": "0.0.1",
  "title": "ShipStation",
  "description": "ShipStation API",
  "settingsSchema": {
    "title": "ShipStation",
    "type": "object",
    "properties": {
      "apiKey": {
        "title": "API Key",
        "description": "ShipStation API Key",
        "type": "string"
      },
      "apiSecret": {
        "title": "API Secret",
        "description": "ShipStation API Secret",
        "type": "string"
      },
      "appKey": {
        "title": "App Key",
        "description": "Vtex App Key",
        "type": "string"
      },
      "appToken": {
        "title": "App Token",
        "description": "Vtex App Token",
        "type": "string"
      }
    }
  },
  "builders": {
    "dotnet": "2.x",
    "docs": "0.x"
  },
  "mustUpdateAt": "2018-09-05",
  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "registries": [
    "smartcheckout"
  ],
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "bnb.data.bl.uk"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "nuget.org"
      }
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "AcessaTodosPedidos"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/checkout/pvt/orders/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/orders/hook/config"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "ssapi.shipstation.com",
        "path": "/*"
      }
    },
	{
      "name": "outbound-access",
      "attrs": {
        "host": "ssapi12.shipstation.com",
        "path": "shipments"
      }
    },
    {
      "name": "vbase-read-write"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
